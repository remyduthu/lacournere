{
  "private": true,
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.0",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run build && lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,js,html}": "prettier --write"
  },
  "scripts": {
    "build": "postcss styles.css -o static/dist/styles.css"
  }
}
